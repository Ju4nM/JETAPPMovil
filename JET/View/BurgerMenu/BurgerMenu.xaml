<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="JET.View.BurgerMenu.BurgerMenu"
             xmlns:pages="clr-namespace:JET.View.BurgerMenu"
             xmlns:initial="clr-namespace:JET.View.DashboardViews"
            NavigationPage.HasNavigationBar="false">
  <FlyoutPage.Flyout>
    <pages:BurgerMenuFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <initial:AboutView />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>